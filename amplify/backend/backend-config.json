{
  "api": {
    "reactamplified": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 100,
              "description": "demo"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "stripeapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "stripeCheckout",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "stripeWebhook",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "function": {
    "stripeCheckout": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "stripeWebhook": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  }
}